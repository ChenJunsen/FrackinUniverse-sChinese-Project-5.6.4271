    "Files": {
      "npcs/dungeon/pyramidguard.npctype": [
        "/scriptConfig/dialog/attack/avian/default/9"
      ]
    },
    "Texts": {
      "Chs": "在我的武器下灭亡吧...",
      "Eng": "Die by my sword..."
    }
  },
  {
    "DeniedAlternatives": [],
    "Files": {
      "npcs/dungeon/pyramidguard.npctype": [
        "/scriptConfig/dialog/attack/avian/avian/5"
      ]
    },
    "Texts": {
      "Chs": "滚出去...你不能踏入此地...",
      "Eng": "Get out... you are forbidden..."
