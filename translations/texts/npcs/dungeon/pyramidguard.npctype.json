  {
    "DeniedAlternatives": [],
    "Files": {
      "npcs/dungeon/pyramidguard.npctype": [
        "/scriptConfig/dialog/attack/avian/default/6"
      ]
    },
    "Texts": {
      "Chs": "小偷...克鲁西斯会审判你的...",
      "Eng": "Thief... Kluex will strike you down..."
