  {
    "DeniedAlternatives": [],
    "Files": {
      "npcs/dungeon/pyramidguard.npctype": [
        "/scriptConfig/dialog/attack/avian/default/1"
      ]
    },
    "Texts": {
      "Chs": "我必须...保护这里...",
      "Eng": "I must... defend. I am compelled..."
    }
  },
  {
    "DeniedAlternatives": [],
    "Files": {
      "npcs/dungeon/pyramidguard.npctype": [
        "/scriptConfig/dialog/attack/avian/avian/8"
      ]
    },
    "Texts": {
      "Chs": "我...我还记得阿沃斯...",
      "Eng": "I... I remember Avos..."
