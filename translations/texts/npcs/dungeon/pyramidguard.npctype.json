  },
  {
    "DeniedAlternatives": [],
    "Files": {
      "npcs/dungeon/pyramidguard.npctype": [
        "/scriptConfig/dialog/attack/avian/avian/7"
      ]
    },
    "Texts": {
      "Chs": "你这个亵渎者...",
